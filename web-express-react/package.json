{
  "name": "browser-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/express": "^4.16.0",
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts-ts": "2.15.1"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "server": "ts-node -P tsconfig.server.json src/server",
    "build-browser": "react-scripts-ts build",
    "build-server": "tsc "
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.6",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "require-in-the-middle": "^2.2.1",
    "ts-node": "^6.1.0",
    "typescript": "^2.8.3"
  },
  "proxy": {
      "/api": {
        "target": "http://localhost:3001",
        "ws": true
      },
      "/socket.io": {
        "target": "http://localhost:3001",
        "ws": true
      },
      "/sockjs-node": {
        "target": "wss://localhost:3001",
        "ws": true
      }
    }
}
