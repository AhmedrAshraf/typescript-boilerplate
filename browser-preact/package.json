{
  "name": "browser-preact",
  "version": "0.1.0",
  "author": "Wang Guan <momocraft@gmail.com>",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "test": "CI=true react-scripts test",
    "test:watch": "react-scripts test --watch",
    "test:coverage": "CI=true react-scripts test --env=jsdom --coverage --watchAll=false --coverageReporters=json --coverageReporters=lcov --coverageReporters=text --coverageReporters=html",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "    eslint '**/*.@(ts|js|tsx|jsx)'",
    "lint:fix": "eslint '**/*.@(ts|js|tsx|jsx)' --fix"
  },
  "dependencies": {
    "preact": "^10.4.1",
    "tslib": "^1.11.2"
  },
  "devDependencies": {
    "@types/jest": "^25",
    "@types/node": "^12",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^25",
    "prettier": "^2.0.5",
    "react-scripts": "3.4.1",
    "sass": "^1.26.5",
    "source-map-explorer": "^2.4.2",
    "ts-jest": "^25",
    "typescript": "^3.8"
  }
}
